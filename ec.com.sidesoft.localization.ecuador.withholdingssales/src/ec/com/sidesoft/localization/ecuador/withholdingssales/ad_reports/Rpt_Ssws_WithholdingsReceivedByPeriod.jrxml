	<queryString>
		<![CDATA[(
SELECT
    po.name organization
    , po.ad_org_id organization_id
    , TO_CHAR(NOW(), 'dd/MM/yyyy HH24:MI:SS') printing_date
    , pu.name printing_user
    , TO_CHAR($P{DateFrom}::DATE, 'dd/MM/yyyy') datefrom
    , TO_CHAR($P{DateTo}::DATE, 'dd/MM/yyyy') dateto
    , ws.documentno ws_docno
    , ws.withholdingdate
    , ws.numauto
    , bp.name bp_name
    , i.documentno i_docno
    , i.dateinvoiced
    , CASE WHEN COALESCE(i.ispaid, 'N') = 'Y' THEN 'Si' ELSE 'No' END ispaid
    , it.it_tax_base
    , it.it_retention_perc
    , it.it_retained_value
    , vat.vat_tax_base
    , vat.vat_retention_perc
    , vat.vat_retained_value
FROM ssws_withholdingsale ws
    JOIN ad_org o ON o.ad_org_id = ws.ad_org_id
    JOIN c_bpartner bp ON bp.c_bpartner_id = ws.c_bpartner_id
    JOIN c_invoice i ON i.c_invoice_id = ws.c_invoice_id
    LEFT JOIN (
        SELECT wsl.ssws_withholdingsale_id
            , SUM(wsl.linenetamt) it_tax_base
            , ABS(tax.rate) it_retention_perc
            , SUM(wsl.whrentalamt) it_retained_value
        FROM ssws_withholdingsaleline wsl
            JOIN c_tax tax on tax.c_tax_id = wsl.c_tax_id
        WHERE wsl.isrental = 'Y'
        GROUP BY wsl.ssws_withholdingsale_id, tax.rate
    ) it ON it.ssws_withholdingsale_id = ws.ssws_withholdingsale_id
    LEFT JOIN (
        SELECT wsl.ssws_withholdingsale_id
            , SUM(wsl.lineivaamt) vat_tax_base
            , ABS(tax.rate) vat_retention_perc
            , SUM(wsl.whivaamt) vat_retained_value
        FROM ssws_withholdingsaleline wsl
            JOIN c_tax tax on tax.c_tax_id = wsl.c_tax_id
        WHERE wsl.isrental = 'N'
        GROUP BY wsl.ssws_withholdingsale_id, tax.rate
    ) vat ON vat.ssws_withholdingsale_id = ws.ssws_withholdingsale_id

    LEFT JOIN ad_org po ON po.issummary = 'Y' AND po.ad_orgtype_id = '1'
    JOIN ad_user pu ON pu.ad_user_id = $P{AD_User_ID}
WHERE ws.docstatus = 'CO'
    AND ad_isorgincluded(o.ad_org_id, $P{AD_Org_ID}, o.ad_client_id) > 0
    AND ws.withholdingdate::DATE >= $P{DateFrom}::DATE
    AND ws.withholdingdate::DATE <= $P{DateTo}::DATE
    AND ($P{IsPosted} = ws.posted OR $P{IsPosted} IS NULL)
ORDER BY ws.withholdingdate, bp_name
        )]]>
	</queryString>
