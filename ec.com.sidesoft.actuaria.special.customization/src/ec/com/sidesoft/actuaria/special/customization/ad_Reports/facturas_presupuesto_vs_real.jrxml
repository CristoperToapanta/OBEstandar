	<queryString>
		<![CDATA[select
b.c_year_id,cy.year,u1.name concepto_grupo, pr.name concepto_hijo, pr.value codigo_hijo,
sum(amount) as presupuesto, sum(real_factura) as real_factura,sum(real_nc) as real_nc,
now() as printing_date,ao.ad_org_id as org_id,
sum(real_factura-real_nc) as real,
CASE
    WHEN sum(amount) = 0 THEN 0
	WHEN sum(amount) <>0 then ((sum(real_factura-real_nc))/(sum(amount)))*100
  END AS dif_porc_r_p
from c_budget b
join c_budgetline bl on b.c_budget_id = bl.c_budget_id
join c_elementvalue ev on bl.c_elementvalue_id = ev.c_elementvalue_id
join c_period p on bl.c_period_id = p.c_period_id
join m_product pr on pr.m_product_id = bl.m_product_id
join user1 u1 on u1.user1_id = bl.user1_id
join ad_org ao on ao.ad_orgtype_id = '1'
join c_costcenter cc on bl.c_costcenter_id = cc.c_costcenter_id
join c_year cy on cy.c_year_id=b.c_year_id
LEFT JOIN
(
	select
		c_period.c_period_id,
		c_invoice.user1_id,
		c_invoice.c_costcenter_id,
		m_product_id,
		SUM(case when docbasetype in ('ARI') then linenetamt
			else 0
		end) as real_factura,
		SUM(case when docbasetype in ('ARI') then 0
			else abs(linenetamt)
		end) as real_nc
	from c_invoice, c_doctype, c_invoiceline, c_period
	where c_invoice.c_doctype_id = c_doctype.c_doctype_id and c_invoice.c_invoice_id = c_invoiceline.c_invoice_id
	and dateinvoiced between $P{Datefrom} AND $P{Dateto}
	and c_invoice.issotrx = 'Y'
	and docstatus = 'CO'
	group by c_period.c_period_id,
		c_invoice.user1_id,
		c_invoice.c_costcenter_id,
		m_product_id
) X ON X.c_period_id = p.c_period_id
where ev.accounttype = 'R'
and X.user1_id = u1.user1_id
and X.c_costcenter_id = cc.c_costcenter_id
and X.m_product_id = pr.m_product_id
AND x.c_period_id = p.c_period_id
and b.c_year_id = $P{C_YEAR_ID}
and $P{Datefrom}::date <= p.startdate::date
and $P{Dateto}::date >= p.enddate::date
group by b.c_year_id,cy.year,u1.name, pr.name, pr.value, ao.ad_org_id]]>
	</queryString>
